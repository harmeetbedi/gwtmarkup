<?xml version="1.0" encoding="UTF-8"?><module>		

	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name="com.extjs.gxt.ui.GXT"/>

    <!-- Inherit the GWT Widgets library                      -->
    <inherits name='org.gwtwidgets.WidgetLibrary'/>

    <!-- Inherit the GWT Visualization library for graphs                      -->
    <inherits name="com.google.gwt.visualization.Visualization" />
<!--
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.junit.JUnit"/>
-->
	
<!--
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<inherits name="com.google.gwt.i18n.I18N"/>
-->

	<!-- Set user.agent so we only target one of the 5 browsers (for developer productivity) -->
<!-- 
	<set-property name="user.agent" value="safari" />
 -->
 
	<!-- Specify the app's entry point class.                       -->
    <entry-point class="emforium.gwt.widget.GwtWidgetMarkup"/>


	<!-- Resources GWT will expose via the web service.             -->
	<public path="public"/>
    <source path="widget"/>
    <source path="util"/>

	<!-- Specify the application specific style sheet.              -->
    
</module>
